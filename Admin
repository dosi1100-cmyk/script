-- [ì±„íŒ… ë¡œê·¸ ì „ì†¡ í•¨ìˆ˜]
local function sendDiscordMessage(player, message)
    if not http_request then return end
    
    local payload = {
        content = "",
        embeds = {{
            title = "ğŸ’¬ ì±„íŒ… ë¡œê·¸ ê°ì§€",
            description = "**í”Œë ˆì´ì–´:** " .. player.DisplayName .. " (@" .. player.Name .. ")\n**ë©”ì‹œì§€:** " .. message,
            color = 3447003,
            footer = {
                text = "Place ID: " .. tostring(game.PlaceId) .. " | Job ID: " .. tostring(game.JobId)
            },
            timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
        }}
    }

    pcall(function()
        http_request({
            Url = chatWebhookUrl, -- ì±„íŒ… ë¡œê·¸ìš© ì›¹í›… ì‚¬ìš©
            Method = "POST",
            Headers = { ["Content-Type"] = "application/json" },
            Body = HttpService:JSONEncode(payload)
        })
    end)
end

-- [ì…í‡´ì¥ ë¡œê·¸ ì „ì†¡ í•¨ìˆ˜]
local function sendJoinLeaveMessage(player, type)
    if not http_request then return end
    
    local titleText = ""
    local colorCode = 0
    local descText = ""

    if type == "Join" then
        titleText = "ğŸŸ¢ ì„œë²„ ì ‘ì† ê°ì§€"
        colorCode = 65280 -- Green
        descText = "**" .. player.DisplayName .. " (@" .. player.Name .. ")** ë‹˜ì´ ì„œë²„ì— ì ‘ì†í–ˆìŠµë‹ˆë‹¤."
    else
        titleText = "ğŸ”´ ì„œë²„ í‡´ì¥ ê°ì§€"
        colorCode = 16711680 -- Red
        descText = "**" .. player.DisplayName .. " (@" .. player.Name .. ")** ë‹˜ì´ ì„œë²„ì—ì„œ ë‚˜ê°”ìŠµë‹ˆë‹¤."
    end

    local payload = {
        content = "",
        embeds = {{
            title = titleText,
            description = descText,
            color = colorCode,
            footer = {
                text = "í˜„ì¬ ì¸ì›: " .. tostring(#Players:GetPlayers()) .. "ëª… | Place ID: " .. tostring(game.PlaceId)
            },
            timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
        }}
    }

    pcall(function()
        http_request({
            Url = joinWebhookUrl, -- ì…í‡´ì¥ ë¡œê·¸ìš© ì›¹í›… ì‚¬ìš©
            Method = "POST",
            Headers = { ["Content-Type"] = "application/json" },
            Body = HttpService:JSONEncode(payload)
        })
    end)
end

-- [ì±„íŒ… ë¡œê·¸ ì¼œê¸°/ë„ê¸° í•¨ìˆ˜]
local function toggleChatLog(enable)
    if enable then
        if chatLogActive then return end
        chatLogActive = true
        
        local function hookPlayer(p)
            if chatLogConnections[p] then return end
            local conn = p.Chatted:Connect(function(msg)
                if chatLogActive then
                    sendDiscordMessage(p, msg)
                end
            end)
            chatLogConnections[p] = conn
        end

        for _, p in ipairs(Players:GetPlayers()) do
            hookPlayer(p)
        end

        chatLogConnections["_PlayerAdded"] = Players.PlayerAdded:Connect(function(p)
            hookPlayer(p)
        end)
        
        chatLogConnections["_PlayerRemoving"] = Players.PlayerRemoving:Connect(function(p)
            if chatLogConnections[p] then
                chatLogConnections[p]:Disconnect()
                chatLogConnections[p] = nil
            end
        end)

        StarterGui:SetCore("SendNotification", {Title = "ì±„íŒ… ë¡œê·¸ ON", Text = "ëª¨ë“  ì±„íŒ…ì„ ë””ìŠ¤ì½”ë“œ ì›¹í›…ìœ¼ë¡œ ì „ì†¡í•©ë‹ˆë‹¤.", Duration = 3})
    else
        chatLogActive = false
        for key, conn in pairs(chatLogConnections) do
            if conn then conn:Disconnect() end
        end
        chatLogConnections = {}
        StarterGui:SetCore("SendNotification", {Title = "ì±„íŒ… ë¡œê·¸ OFF", Text = "ì „ì†¡ì„ ì¤‘ë‹¨í–ˆìŠµë‹ˆë‹¤.", Duration = 3})
    end
end

-- [Noclip í† ê¸€ í•¨ìˆ˜ (WalkFling ì—°ë™ìš©)]
local function toggleNoclip(state)
    if state then
        if noclipConnection then noclipConnection:Disconnect() end
        noclipConnection = RunService.Stepped:Connect(function()
            if lp.Character then
                for _, v in pairs(lp.Character:GetDescendants()) do
                    if v:IsA("BasePart") and v.CanCollide then
                        v.CanCollide = false
                    end
                end
            end
        end)
    else
        if noclipConnection then
            noclipConnection:Disconnect()
            noclipConnection = nil
        end
    end
end

-- [Fly ê¸°ëŠ¥ í†µí•© ì œì–´ í•¨ìˆ˜]
local function stopFlying()
    if flyConnection then flyConnection:Disconnect() flyConnection = nil end
    if flyBodyVelocity then flyBodyVelocity:Destroy() flyBodyVelocity = nil end
    if flyBodyGyro then flyBodyGyro:Destroy() flyBodyGyro = nil end
    
    if lp.Character and lp.Character:FindFirstChild("Humanoid") then
        lp.Character.Humanoid.PlatformStand = false
    end
    currentFlyType = nil
end

local function startFlying(mode, speedInput)
    stopFlying() -- ê¸°ì¡´ í”Œë¼ì´ê°€ ìˆë‹¤ë©´ ë„ê³  ìƒˆë¡œ ì‹œì‘
    
    currentFlyType = mode

    -- ì†ë„ ì²˜ë¦¬: ì¸ìê°€ ìˆìœ¼ë©´ ìˆ«ì * 10, ì—†ìœ¼ë©´ ê¸°ë³¸ 50
    local desired = tonumber(speedInput)
    if desired then
        flySpeed = desired * 10
    else
        flySpeed = 50
    end
    if flySpeed < 1 then flySpeed = 50 end

    local function getFlyDirection(camCF)
        local moveDir = Vector3.new(0,0,0)
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then moveDir = moveDir + camCF.LookVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then moveDir = moveDir - camCF.LookVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then moveDir = moveDir - camCF.RightVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then moveDir = moveDir + camCF.RightVector end
        return moveDir
    end

    flyConnection = RunService.RenderStepped:Connect(function()
        local char = lp.Character
        if not char then return end
        local hum = char:FindFirstChild("Humanoid")
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if not hum or not hrp then return end

        local targetPart = hrp
        local isInVehicle = false

        -- VFly ëª¨ë“œì¼ ë•Œë§Œ ì°¨ëŸ‰ íƒ‘ìŠ¹ ì—¬ë¶€ ì²´í¬
        if mode == "vfly" and hum.SeatPart then
            targetPart = hum.SeatPart -- ì°¨ëŸ‰(Seat)ì„ íƒ€ê²Ÿìœ¼ë¡œ ì„¤ì •
            isInVehicle = true
        end

        -- ë¬¼ë¦¬ ê°ì²´(BV, BG)ê°€ í˜„ì¬ íƒ€ê²Ÿì— ì—†ìœ¼ë©´ ìƒì„± (ì°¨ëŸ‰ <-> ìºë¦­í„° ì „í™˜ ì‹œ ìë™ ì²˜ë¦¬)
        if not targetPart:FindFirstChild("MyFlyVelocity") then
            -- ê¸°ì¡´ì— ë‹¤ë¥¸ íŒŒíŠ¸ì— ë¶™ì–´ìˆë˜ê±° ì‚­ì œ
            if flyBodyVelocity then flyBodyVelocity:Destroy() end
            if flyBodyGyro then flyBodyGyro:Destroy() end

            local bv = Instance.new("BodyVelocity")
            bv.Name = "MyFlyVelocity"
            bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            bv.Velocity = Vector3.new(0,0,0)
            bv.Parent = targetPart
            flyBodyVelocity = bv

            local bg = Instance.new("BodyGyro")
            bg.Name = "MyFlyGyro"
            bg.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
            bg.P = 9e4
            bg.Parent = targetPart
            flyBodyGyro = bg
        end

        -- ì¹´ë©”ë¼ ë° ì´ë™ ë°©í–¥ ê³„ì‚°
        local cam = workspace.CurrentCamera
        local moveDir = getFlyDirection(cam.CFrame)

        -- ë¬¼ë¦¬ ì ìš©
        if flyBodyVelocity and flyBodyGyro then
            flyBodyVelocity.Velocity = moveDir * flySpeed
            flyBodyGyro.CFrame = cam.CFrame
        end

        -- ì• ë‹ˆë©”ì´ì…˜/ìƒíƒœ ì²˜ë¦¬
        if hum then
            if isInVehicle then
                hum.PlatformStand = false -- ì°¨ì— íƒ”ì„ ë• ì•‰ì€ ìì„¸ ìœ ì§€
            else
                hum.PlatformStand = true -- ê·¸ëƒ¥ ë‚  ë• ì„œìˆëŠ” ìƒíƒœ í•´ì œ(ë‚ ê¸° í¬ì¦ˆ)
            end
        end
    end)
    
    local titleText = (mode == "vfly") and "VFly í™œì„±í™”" or "Fly í™œì„±í™”"
    StarterGui:SetCore("SendNotification", {Title = titleText, Text = "ì†ë„: " .. flySpeed, Duration = 3})
end


-- [ëª…ë ¹ì–´ ì²˜ë¦¬ í†µí•© í•¨ìˆ˜]
local function processCommand(msg)
    -- ê³µë°± ì œê±°
    msg = msg:gsub("^%s+", ""):gsub("%s+$", "")
    if #msg == 0 then return end

    local lowerMsg = msg:lower()

    if lowerMsg == ".ì¬ì ‘" or lowerMsg == ".rj" then
        pcall(function()
            if #Players:GetPlayers() <= 1 then
                TeleportService:Teleport(game.PlaceId, lp)
            else
                TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId, lp)
            end
        end)

    elseif lowerMsg == ".ë ‰" or lowerMsg == ".lag" or lowerMsg:find("ë ‰%.") or lowerMsg:find("lag%.") then
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/dosi1100-cmyk/script/refs/heads/main/ServerLag"))()
        end)

    elseif lowerMsg == ".ì—ì„ë´‡" or lowerMsg == ".aimbot" then
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/dosi1100-cmyk/script/refs/heads/main/aimbot"))()
        end)

    elseif lowerMsg == ".ë„ì›€" or lowerMsg == ".help" then
        if helpFrame then
            helpFrame.Visible = not helpFrame.Visible
        end
        
    elseif lowerMsg == ".reset" or lowerMsg == ".die" then
        if lp.Character then
            local head = lp.Character:FindFirstChild("Head")
            if head then head:Destroy() end
            lp.Character:BreakJoints()
            local hum = lp.Character:FindFirstChild("Humanoid")
            if hum then 
                hum.Health = -100 
                hum:ChangeState(Enum.HumanoidStateType.Dead)
            end
            StarterGui:SetCore("SendNotification", {Title = "ì‚¬ë§ ì²˜ë¦¬", Text = "ìºë¦­í„°ë¥¼ ê°•ì œ ì‚¬ë§ì‹œì¼°ìŠµë‹ˆë‹¤.", Duration = 2})
        end

    elseif lowerMsg == ".í•‘" then
        local ping = math.floor(lp:GetNetworkPing() * 1000)
        local pingGui = Instance.new("ScreenGui")
        pingGui.Name = "PingNotification"
        pingGui.Parent = lp:WaitForChild("PlayerGui")
        pingGui.ResetOnSpawn = false
        pingGui.DisplayOrder = 999999998

        local pingLabel = Instance.new("TextLabel")
        pingLabel.Size = UDim2.new(0, 300, 0, 50)
        pingLabel.Position = UDim2.new(1, 320, 1, -70)
        pingLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        pingLabel.BackgroundTransparency = 0.5
        pingLabel.Text = "í˜„ì¬ í•‘ì€ " .. ping .. "ms ì…ë‹ˆë‹¤."
        pingLabel.TextColor3 = Color3.new(1, 1, 1)
        pingLabel.Font = Enum.Font.GothamBold
        pingLabel.TextSize = 20
        pingLabel.Parent = pingGui

        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 10)
        corner.Parent = pingLabel

        local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quart, Enum.EasingDirection.Out)
        local tween = TweenService:Create(pingLabel, tweenInfo, {Position = UDim2.new(1, -320, 1, -70)})
        tween:Play()

        wait(3)
        local fadeTween = TweenService:Create(pingLabel, TweenInfo.new(0.5), {TextTransparency = 1, BackgroundTransparency = 1})
        fadeTween:Play()
        fadeTween.Completed:Wait()
        pingGui:Destroy()

    elseif lowerMsg == ".ì¹œêµ¬" then
        local friendsCount = 0
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= lp and lp:IsFriendsWith(player.UserId) then
                friendsCount = friendsCount + 1
            end
        end
        local friendGui = Instance.new("ScreenGui")
        friendGui.Name = "FriendNotification"
        friendGui.Parent = lp:WaitForChild("PlayerGui")
        friendGui.ResetOnSpawn = false
        friendGui.DisplayOrder = 999999997

        local friendLabel = Instance.new("TextLabel")
        friendLabel.Size = UDim2.new(0, 300, 0, 50)
        friendLabel.Position = UDim2.new(1, 320, 1, -70)
        friendLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        friendLabel.BackgroundTransparency = 0.5
        friendLabel.Text = "ì„œë²„ ë‚´ ì¹œêµ¬ ìˆ˜: " .. friendsCount .. "ëª…"
        friendLabel.TextColor3 = Color3.new(1, 1, 1)
        friendLabel.Font = Enum.Font.GothamBold
        friendLabel.TextSize = 20
        friendLabel.Parent = friendGui

        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 10)
        corner.Parent = friendLabel

        local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quart, Enum.EasingDirection.Out)
        local tween = TweenService:Create(friendLabel, tweenInfo, {Position = UDim2.new(1, -320, 1, -70)})
        tween:Play()

        wait(3)
        local fadeTween = TweenService:Create(friendLabel, TweenInfo.new(0.5), {TextTransparency = 1, BackgroundTransparency = 1})
        fadeTween:Play()
        fadeTween.Completed:Wait()
        friendGui:Destroy()

    elseif lowerMsg == ".ì„œë²„ ì •ë³´" or lowerMsg == ".svinfo" or lowerMsg == ".ì„œë²„ì •ë³´" or lowerMsg == ".ì„œë²„ ì •ë³´ë¥¼" then
        if not serverInfoGui or not serverInfoGui.Parent then
            createServerInfoGui()
        else
            serverInfoGui.Enabled = not serverInfoGui.Enabled
        end

    elseif lowerMsg:sub(1, 6) == ".join " then
        local targetNamePart = lowerMsg:sub(7)
        local success, onlineFriends = pcall(function() 
            return lp:GetFriendsOnline(200) 
        end)
        
        if not success then
             StarterGui:SetCore("SendNotification", {Title = "ì˜¤ë¥˜", Text = "ì¹œêµ¬ ëª©ë¡ì„ ê°€ì ¸ì˜¤ëŠ”ë° ì‹¤íŒ¨í–ˆìŠµë‹ˆë‹¤.", Duration = 3})
        else
            local foundFriend = nil
            for _, friend in ipairs(onlineFriends) do
                local fName = friend.UserName:lower()
                local fDisplay = friend.DisplayName:lower()
                if fName:sub(1, #targetNamePart) == targetNamePart or fDisplay:sub(1, #targetNamePart) == targetNamePart then
                    foundFriend = friend
                    break
                end
            end

            if foundFriend then
                StarterGui:SetCore("SendNotification", {Title = "ì ‘ì† ì‹œë„", Text = foundFriend.UserName .. "ë‹˜ì—ê²Œ ì´ë™í•©ë‹ˆë‹¤...", Duration = 3})
                local placeId = foundFriend.PlaceId
                if placeId then
                    if placeId ~= game.PlaceId then
                        StarterGui:SetCore("SendNotification", {Title = "ê²Œì„ ì´ë™", Text = "ë‹¤ë¥¸ ê²Œì„ ì ‘ì† ì¤‘ (ëŒ€ê¸°ì—´ ìë™ ì§„ì…)", Duration = 4})
                        TeleportService:Teleport(placeId, lp)
                    else
                        local didTeleport = false
                        pcall(function()
                            if foundFriend.GameId then
                                TeleportService:TeleportToPlaceInstance(placeId, foundFriend.GameId, lp)
                                didTeleport = true
                            end
                        end)
                        if not didTeleport then
                             TeleportService:Teleport(placeId, lp)
                        end
                    end
                else
                    StarterGui:SetCore("SendNotification", {Title = "ì˜¤ë¥˜", Text = "ì¹œêµ¬ì˜ ìœ„ì¹˜ ì •ë³´ë¥¼ ê°€ì ¸ì˜¬ ìˆ˜ ì—†ìŠµë‹ˆë‹¤.", Duration = 3})
                end
            else
                StarterGui:SetCore("SendNotification", {Title = "ì°¾ì„ ìˆ˜ ì—†ìŒ", Text = "ì¹œêµ¬ê°€ ì˜¤í”„ë¼ì¸ì´ê±°ë‚˜ ì°¾ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤.", Duration = 3})
            end
        end

    elseif lowerMsg == ".jobid" then
        setclipboard(tostring(game.JobId))
        StarterGui:SetCore("SendNotification", {Title = "ë³µì‚¬ ì™„ë£Œ", Text = "í˜„ì¬ ì„œë²„ì˜ Job IDê°€ í´ë¦½ë³´ë“œì— ë³µì‚¬ë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 2})

    elseif lowerMsg:sub(1, 5) == ".age " then
        local targetNamePart = lowerMsg:sub(6)
        local targetP = nil
        for _, p in ipairs(Players:GetPlayers()) do
            local pName = p.Name:lower()
            local pDisplay = p.DisplayName:lower()
            if pName:sub(1, #targetNamePart) == targetNamePart or pDisplay:sub(1, #targetNamePart) == targetNamePart then
                targetP = p
                break
            end
        end

        if targetP then
            local days = targetP.AccountAge
            local createdTime = os.time() - (days * 86400)
            local createdDate = os.date("%Yë…„ %mì›” %dì¼", createdTime)
            local years = math.floor(days / 365)
            local leftDays = days % 365
            local months = math.floor(leftDays / 30)
            local approxStr = ""
            if years > 0 then approxStr = years .. "ë…„ " end
            if months > 0 then approxStr = approxStr .. months .. "ê°œì›”" end
            if years == 0 and months == 0 then approxStr = "1ê°œì›” ë¯¸ë§Œ" end
            
            StarterGui:SetCore("SendNotification", {
                Title = targetP.Name .. "ë‹˜ì˜ ì •ë³´",
                Text = "ê³„ì • ë‚˜ì´: " .. days .. "ì¼ (ì•½ " .. approxStr .. ")\nê°€ì…ì¼: " .. createdDate,
                Duration = 6
            })
        else
            StarterGui:SetCore("SendNotification", {Title = "ì˜¤ë¥˜", Text = "ì„œë²„ ë‚´ì—ì„œ í•´ë‹¹ í”Œë ˆì´ì–´ë¥¼ ì°¾ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤.", Duration = 3})
        end

    elseif lowerMsg == ".fullbright" or lowerMsg == ".fb" then
        if not fullbrightLoop then
            savedLightingSettings = {
                Brightness = Lighting.Brightness,
                ClockTime = Lighting.ClockTime,
                FogEnd = Lighting.FogEnd,
                GlobalShadows = Lighting.GlobalShadows,
                OutdoorAmbient = Lighting.OutdoorAmbient,
                Ambient = Lighting.Ambient
            }

            fullbrightLoop = RunService.RenderStepped:Connect(function()
                Lighting.Brightness = 2
                Lighting.ClockTime = 13
                Lighting.FogEnd = 70000
                Lighting.GlobalShadows = false
                Lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
                Lighting.Ambient = Color3.fromRGB(255, 255, 255)
            end)
            StarterGui:SetCore("SendNotification", {Title = "Fullbright ì¼œì§", Text = "ë§µì´ ìµœëŒ€ ë°ê¸°ë¡œ ìœ ì§€ë©ë‹ˆë‹¤.", Duration = 3})
        else
            fullbrightLoop:Disconnect()
            fullbrightLoop = nil
            
            if savedLightingSettings.Brightness then
                Lighting.Brightness = savedLightingSettings.Brightness
                Lighting.ClockTime = savedLightingSettings.ClockTime
                Lighting.FogEnd = savedLightingSettings.FogEnd
                Lighting.GlobalShadows = savedLightingSettings.GlobalShadows
                Lighting.OutdoorAmbient = savedLightingSettings.OutdoorAmbient
                Lighting.Ambient = savedLightingSettings.Ambient
            end
            StarterGui:SetCore("SendNotification", {Title = "Fullbright êº¼ì§", Text = "ì›ë˜ ë°ê¸°ë¡œ ë³µêµ¬ë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 3})
        end

    elseif lowerMsg == ".unfullbright" or lowerMsg == ".unfb" then
        if fullbrightLoop then
            fullbrightLoop:Disconnect()
            fullbrightLoop = nil

            if savedLightingSettings.Brightness then
                Lighting.Brightness = savedLightingSettings.Brightness
                Lighting.ClockTime = savedLightingSettings.ClockTime
                Lighting.FogEnd = savedLightingSettings.FogEnd
                Lighting.GlobalShadows = savedLightingSettings.GlobalShadows
                Lighting.OutdoorAmbient = savedLightingSettings.OutdoorAmbient
                Lighting.Ambient = savedLightingSettings.Ambient
            else
                Lighting.Brightness = 1
                Lighting.GlobalShadows = true
            end
            
            StarterGui:SetCore("SendNotification", {Title = "Fullbright í•´ì œ", Text = "ë°ê¸°ë¥¼ ì›ë˜ëŒ€ë¡œ ë˜ëŒë ¸ìŠµë‹ˆë‹¤.", Duration = 3})
        else
            StarterGui:SetCore("SendNotification", {Title = "ì•Œë¦¼", Text = "Fullbrightê°€ ì¼œì ¸ìˆì§€ ì•ŠìŠµë‹ˆë‹¤.", Duration = 2})
        end

    elseif lowerMsg == ".nofog" then
        Lighting.FogEnd = 1000000 
        Lighting.FogStart = 0
        for _, v in pairs(Lighting:GetChildren()) do
            if v:IsA("Atmosphere") then
                v:Destroy()
            end
        end
        StarterGui:SetCore("SendNotification", {Title = "ì•ˆê°œ ì œê±° ì™„ë£Œ", Text = "ì•ˆê°œì™€ Atmosphereë¥¼ ì œê±°í–ˆìŠµë‹ˆë‹¤.", Duration = 3})

    elseif lowerMsg == ".infjump" then
        if not infJumpConnection then
            if lp.Character and lp.Character:FindFirstChild("Humanoid") then
                local hum = lp.Character:FindFirstChild("Humanoid")
                previousJumpPower = hum.JumpPower
            else
                previousJumpPower = nil
            end

            infJumpConnection = UserInputService.JumpRequest:Connect(function()
                if lp.Character and lp.Character:FindFirstChild("Humanoid") then
                    lp.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                end
            end)
            StarterGui:SetCore("SendNotification", {Title = "InfJump ì¼œì§", Text = "ê³µì¤‘ ì í”„ê°€ ê°€ëŠ¥í•©ë‹ˆë‹¤.", Duration = 3})
        else
            StarterGui:SetCore("SendNotification", {Title = "InfJump", Text = "ì´ë¯¸ ì‹¤í–‰ ì¤‘ì…ë‹ˆë‹¤.", Duration = 3})
        end

    elseif lowerMsg == ".uninfjump" then
        if infJumpConnection then
            infJumpConnection:Disconnect()
            infJumpConnection = nil

            if previousJumpPower and lp.Character and lp.Character:FindFirstChild("Humanoid") then
                pcall(function()
                    lp.Character.Humanoid.JumpPower = previousJumpPower
                end)
            end
            previousJumpPower = nil

            StarterGui:SetCore("SendNotification", {Title = "InfJump í•´ì œ", Text = "ë¬´í•œ ì í”„ê°€ êº¼ì¡ŒìŠµë‹ˆë‹¤. JumpPowerë¥¼ ë³µì›í–ˆìŠµë‹ˆë‹¤.", Duration = 3})
        else
            StarterGui:SetCore("SendNotification", {Title = "ì•Œë¦¼", Text = "ë¬´í•œ ì í”„ê°€ ì¼œì ¸ìˆì§€ ì•ŠìŠµë‹ˆë‹¤.", Duration = 2})
        end

    -- [WalkFling ê¸°ëŠ¥ (Noclip í¬í•¨)]
    elseif lowerMsg == ".walkfling" or lowerMsg == ".wf" then
        if walkflingEnabled then return end
        walkflingEnabled = true

        -- Walkfling ì‚¬ìš© ì¤‘ì—ëŠ” Noclip ìë™ í™œì„±í™”
        toggleNoclip(true)

        StarterGui:SetCore("SendNotification", {Title = "WalkFling í™œì„±í™”", Text = "WalkFling ë° Noclipì´ ì¼œì¡ŒìŠµë‹ˆë‹¤. ìºë¦­í„°ê°€ íšŒì „í•˜ë©° ë‚ ì•„ê°‘ë‹ˆë‹¤.", Duration = 3})

        -- WalkFling ë£¨í”„ ì‹œì‘
        task.spawn(function()
            local movel = 0.1
            while walkflingEnabled do
                RunService.Heartbeat:Wait()
                local character = lp.Character
                local root = character and character:FindFirstChild("HumanoidRootPart")

                if character and character.Parent and root and root.Parent then
                    local vel = root.Velocity
                    root.Velocity = vel * 10000 + Vector3.new(0, 10000, 0)

                    RunService.RenderStepped:Wait()
                    if not walkflingEnabled then break end
                    if character and character.Parent and root and root.Parent then
                        root.Velocity = vel
                    end

                    RunService.Stepped:Wait()
                    if not walkflingEnabled then break end
                    if character and character.Parent and root and root.Parent then
                        root.Velocity = vel + Vector3.new(0, movel, 0)
                        movel = movel * -1
                    end
                else
                    RunService.Heartbeat:Wait()
                end
            end
        end)

    elseif lowerMsg == ".unwalkfling" or lowerMsg == ".unwf" then
        walkflingEnabled = false
        
        -- Walkfling í•´ì œ ì‹œ Noclipë„ í•´ì œ
        toggleNoclip(false)

        -- ì†ë„ ì´ˆê¸°í™”
        if lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") then
            lp.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
        end

        StarterGui:SetCore("SendNotification", {Title = "WalkFling í•´ì œ", Text = "WalkFling ë° Noclipì´ êº¼ì¡ŒìŠµë‹ˆë‹¤.", Duration = 3})

    -- [Noclip ê¸°ëŠ¥ (ë…ë¦½ ì‚¬ìš©)]
    elseif lowerMsg == ".noclip" then
        toggleNoclip(true)
        StarterGui:SetCore("SendNotification", {Title = "Noclip í™œì„±í™”", Text = "ë²½ì„ í†µê³¼í•  ìˆ˜ ìˆìŠµë‹ˆë‹¤.", Duration = 3})

    elseif lowerMsg == ".unnoclip" then
        toggleNoclip(false)
        StarterGui:SetCore("SendNotification", {Title = "Noclip ë¹„í™œì„±í™”", Text = "ë‹¤ì‹œ ë²½ì— ì¶©ëŒí•©ë‹ˆë‹¤.", Duration = 3})

    -- [Fly / VFly ì„¹ì…˜]
    elseif lowerMsg:sub(1, 4) == ".fly" then
        local speedArg = lowerMsg:sub(6)
        startFlying("fly", speedArg)

    elseif lowerMsg:sub(1, 5) == ".vfly" then
        local speedArg = lowerMsg:sub(7)
        startFlying("vfly", speedArg)

    elseif lowerMsg == ".unfly" then
        if currentFlyType == "fly" then
            stopFlying()
            StarterGui:SetCore("SendNotification", {Title = "Fly í•´ì œ", Text = "ë¹„í–‰ì´ ì¢…ë£Œë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 3})
        elseif currentFlyType == "vfly" then
            StarterGui:SetCore("SendNotification", {Title = "ì˜¤ë¥˜", Text = "VFlyë¥¼ ë„ë ¤ë©´ .unvflyë¥¼ ì…ë ¥í•˜ì„¸ìš”.", Duration = 3})
        else
            StarterGui:SetCore("SendNotification", {Title = "ì•Œë¦¼", Text = "ë¹„í–‰ ì¤‘ì´ ì•„ë‹™ë‹ˆë‹¤.", Duration = 2})
        end

    elseif lowerMsg == ".unvfly" then
        if currentFlyType == "vfly" then
            stopFlying()
            StarterGui:SetCore("SendNotification", {Title = "VFly í•´ì œ", Text = "ì°¨ëŸ‰ ë¹„í–‰ì´ ì¢…ë£Œë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 3})
        elseif currentFlyType == "fly" then
            StarterGui:SetCore("SendNotification", {Title = "ì˜¤ë¥˜", Text = "ì¼ë°˜ Flyë¥¼ ë„ë ¤ë©´ .unflyë¥¼ ì…ë ¥í•˜ì„¸ìš”.", Duration = 3})
        else
            StarterGui:SetCore("SendNotification", {Title = "ì•Œë¦¼", Text = "ë¹„í–‰ ì¤‘ì´ ì•„ë‹™ë‹ˆë‹¤.", Duration = 2})
        end

    elseif lowerMsg == ".thirdp" then
        lp.CameraMode = Enum.CameraMode.Classic
        lp.CameraMaxZoomDistance = 999
        lp.CameraMinZoomDistance = 0.5
        StarterGui:SetCore("SendNotification", {Title = "3ì¸ì¹­ í™œì„±í™”", Text = "ì‹œì  ì ê¸ˆì´ í•´ì œë˜ê³  ìµœëŒ€ ì¤Œì´ 999ê°€ ë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 3})

    elseif lowerMsg == ".firstp" then
        lp.CameraMode = Enum.CameraMode.LockFirstPerson
        lp.CameraMaxZoomDistance = 0.5
        StarterGui:SetCore("SendNotification", {Title = "1ì¸ì¹­ í™œì„±í™”", Text = "1ì¸ì¹­ ì‹œì ìœ¼ë¡œ ê°•ì œ ê³ ì •ë©ë‹ˆë‹¤.", Duration = 3})

    elseif lowerMsg:sub(1, 4) == ".to " then
        local targetNamePart = lowerMsg:sub(5)
        local targetPlayer = nil
        
        for _, p in ipairs(Players:GetPlayers()) do
            if p ~= lp then
                local pName = p.Name:lower()
                local pDisplay = p.DisplayName:lower()
                if pName:sub(1, #targetNamePart) == targetNamePart or pDisplay:sub(1, #targetNamePart) == targetNamePart then
                    targetPlayer = p
                    break
                end
            end
        end

        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local targetHRP = targetPlayer.Character.HumanoidRootPart
            local myHRP = lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")
            if myHRP then
                local offsetCFrame = CFrame.new(-5, 0, 5)
                myHRP.AssemblyLinearVelocity = Vector3.new(0, 0, 0) 
                myHRP.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
                myHRP.CFrame = targetHRP.CFrame * offsetCFrame
                myHRP.Anchored = true
                StarterGui:SetCore("SendNotification", {Title = "ì´ë™ ì™„ë£Œ", Text = targetPlayer.Name .. "ë‹˜ì—ê²Œ ì´ë™", Duration = 2})
                task.delay(0.3, function() 
                    if myHRP then myHRP.Anchored = false end 
                end)
            end
        end

    -- [View (ê´€ì „) ê¸°ëŠ¥]
    elseif lowerMsg:sub(1, 6) == ".view " then
        local targetNamePart = lowerMsg:sub(7)
        local targetP = nil
        for _, p in ipairs(Players:GetPlayers()) do
            local pName = p.Name:lower()
            local pDisplay = p.DisplayName:lower()
            if pName:sub(1, #targetNamePart) == targetNamePart or pDisplay:sub(1, #targetNamePart) == targetNamePart then
                targetP = p
                break
            end
        end

        if targetP then
            local workspaceCam = workspace.CurrentCamera
            if targetP.Character and targetP.Character:FindFirstChild("Humanoid") then
                workspaceCam.CameraSubject = targetP.Character.Humanoid
                viewingPlayer = targetP
                StarterGui:SetCore("SendNotification", {Title = "ê´€ì „ ëª¨ë“œ", Text = targetP.Name .. "ë‹˜ì„ ê´€ì „í•©ë‹ˆë‹¤.", Duration = 3})
            else
                StarterGui:SetCore("SendNotification", {Title = "ì˜¤ë¥˜", Text = "í•´ë‹¹ í”Œë ˆì´ì–´ì˜ ìºë¦­í„°ë¥¼ ì°¾ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤.", Duration = 3})
            end
        else
            StarterGui:SetCore("SendNotification", {Title = "ì˜¤ë¥˜", Text = "í•´ë‹¹ í”Œë ˆì´ì–´ë¥¼ ì°¾ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤.", Duration = 3})
        end

    elseif lowerMsg == ".unview" then
        if lp.Character and lp.Character:FindFirstChild("Humanoid") then
            workspace.CurrentCamera.CameraSubject = lp.Character.Humanoid
            viewingPlayer = nil
            StarterGui:SetCore("SendNotification", {Title = "ê´€ì „ ì¢…ë£Œ", Text = "ë‹¤ì‹œ ë³¸ì¸ ì‹œì ìœ¼ë¡œ ëŒì•„ì™”ìŠµë‹ˆë‹¤.", Duration = 3})
        else
            StarterGui:SetCore("SendNotification", {Title = "ì˜¤ë¥˜", Text = "ë³¸ì¸ì˜ ìºë¦­í„°ë¥¼ ì°¾ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤ (ë¦¬ìŠ¤í° ëŒ€ê¸°ì¤‘?).", Duration = 3})
        end

    -- [ì…í‡´ì¥ ë¡œê·¸ ì¼œê¸°]
    elseif lowerMsg == ".joinlog" or lowerMsg == ".jg" then
        if not joinLogActive then
            joinLogActive = true
            StarterGui:SetCore("SendNotification", {Title = "ì…í‡´ì¥ ë¡œê·¸ ON", Text = "ì„œë²„ ì…/í‡´ì¥ ì•Œë¦¼ì„ ì¼°ìŠµë‹ˆë‹¤.", Duration = 3})
        else
            StarterGui:SetCore("SendNotification", {Title = "ì•Œë¦¼", Text = "ì´ë¯¸ ì¼œì ¸ ìˆìŠµë‹ˆë‹¤.", Duration = 2})
        end

    -- [ì…í‡´ì¥ ë¡œê·¸ ë„ê¸°]
    elseif lowerMsg == ".unjoinlog" or lowerMsg == ".unjg" then
        if joinLogActive then
            joinLogActive = false
            StarterGui:SetCore("SendNotification", {Title = "ì…í‡´ì¥ ë¡œê·¸ OFF", Text = "ì„œë²„ ì…/í‡´ì¥ ì•Œë¦¼ì„ ê»ìŠµë‹ˆë‹¤.", Duration = 3})
        else
            StarterGui:SetCore("SendNotification", {Title = "ì•Œë¦¼", Text = "ì´ë¯¸ êº¼ì ¸ ìˆìŠµë‹ˆë‹¤.", Duration = 2})
        end

    elseif lowerMsg == ".spawn" then
        if lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") then
            customSpawnCFrame = lp.Character.HumanoidRootPart.CFrame
            if spawnConnection then spawnConnection:Disconnect() end
            spawnConnection = lp.CharacterAdded:Connect(function(char)
                local hrp = char:WaitForChild("HumanoidRootPart", 10)
                if hrp and customSpawnCFrame then 
                    local forceTime = 0.5
                    local sTime = tick()
                    local conn
                    conn = RunService.Heartbeat:Connect(function()
                        if tick() - sTime > forceTime then
                            conn:Disconnect()
                        elseif hrp then
                            hrp.CFrame = customSpawnCFrame
                            hrp.Velocity = Vector3.new(0,0,0)
                        end
                    end)
                end
            end)
            StarterGui:SetCore("SendNotification", {Title = "ìŠ¤í° ì„¤ì • ì™„ë£Œ", Text = "ì¦‰ì‹œ ìŠ¤í°ìœ¼ë¡œ ì €ì¥ë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 3})
        end
    elseif lowerMsg == ".delspawn" then
        if spawnConnection then spawnConnection:Disconnect() spawnConnection = nil end
        customSpawnCFrame = nil
        StarterGui:SetCore("SendNotification", {Title = "ìŠ¤í° ì‚­ì œ ì™„ë£Œ", Text = "ê¸°ë³¸ ìŠ¤í°ìœ¼ë¡œ ì´ˆê¸°í™”ë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 3})

    elseif lowerMsg == ".dex" then
        pcall(function()
            loadstring(game:HttpGet("https://obj.wearedevs.net/216234/scripts/NDex%20V1.lua"))()
            StarterGui:SetCore("SendNotification", {Title = "Dex ì‹¤í–‰", Text = "Dex Explorer ë¡œë”© ì¤‘...", Duration = 3})
        end)
    
    elseif lowerMsg == ".rspy" then
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/78n/SimpleSpy/main/SimpleSpySource.lua"))()
            StarterGui:SetCore("SendNotification", {Title = "SimpleSpy ì‹¤í–‰", Text = "Remote Spyê°€ ì‹¤í–‰ë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 3})
        end)

    -- [ì±„íŒ… ë¡œê·¸ ëª…ë ¹ì–´]
    elseif lowerMsg == ".chatlog" or lowerMsg == ".cl" then
        toggleChatLog(true)

    elseif lowerMsg == ".unchatlog" or lowerMsg == ".uncl" then
        toggleChatLog(false)

    elseif lowerMsg == ".exit" or lowerMsg == ".run" then
        game:Shutdown()

    elseif lowerMsg == ".ì¢…ë£Œ" or lowerMsg == ".end" then
        StarterGui:SetCore("SendNotification", {
            Title = "ìŠ¤í¬ë¦½íŠ¸ ì¢…ë£Œ",
            Text = "ì—´ë ¤ìˆëŠ” ëª¨ë“  GUIë¥¼ ë‹«ê³  ìŠ¤í¬ë¦½íŠ¸ë¥¼ ì¢…ë£Œí•©ë‹ˆë‹¤.",
            Duration = 4
        })

        -- ì±„íŒ… ëª…ë ¹ ì—°ê²° ì¢…ë£Œ
        if chatConnection then
            chatConnection:Disconnect()
            chatConnection = nil
        end

        -- ìŠ¤í°, í’€ë¸Œë¼ì´íŠ¸, ë¹„í–‰, Noclip, WalkFling ì¢…ë£Œ
        if spawnConnection then spawnConnection:Disconnect() spawnConnection = nil end
        if fullbrightLoop then fullbrightLoop:Disconnect() fullbrightLoop = nil end
        stopFlying()
        toggleNoclip(false) -- Noclip ë° WalkFling ë£¨í”„ ì •ì§€
        walkflingEnabled = false

        -- ë¬´í•œ ì í”„ í•´ì œ
        if infJumpConnection then
            infJumpConnection:Disconnect()
            infJumpConnection = nil
        end

        -- ë¡œê·¸ ê¸°ëŠ¥ ì¢…ë£Œ
        toggleChatLog(false)
        joinLogActive = false

        -- ì„œë²„ ì •ë³´ GUI ì œê±°
        if serverInfoGui then
            serverInfoGui:Destroy()
            serverInfoGui = nil
        end

        -- ë„ì›€ë§ GUI ì œê±°
        if helpGuiGlobal then
            pcall(function() helpGuiGlobal:Destroy() end)
            helpGuiGlobal = nil
        end
        if helpFrame then
            pcall(function() helpFrame:Destroy() end)
            helpFrame = nil
        end

        -- CmdBar ì œê±°
        local cmdGui = lp.PlayerGui:FindFirstChild("CmdBarGui")
        if cmdGui then cmdGui:Destroy() end

        -- ì¹œêµ¬ ì•Œë¦¼ GUI ì œê±°
        if friendAlertGui then
            friendAlertGui:Destroy()
        end

        -- Ping / Friend Notification GUIs ì •ë¦¬
        local toRemove = {"PingNotification", "FriendNotification", "FriendAlerts", "ServerInfoGui", "HelpGui", "CmdBarGui"}
        for _, name in ipairs(toRemove) do
            local g = lp.PlayerGui:FindFirstChild(name)
            if g then
                pcall(function() g:Destroy() end)
            end
        end

        -- ì¶”ê°€ë¡œ PlayerGui ë‚´ì—ì„œ ìœ„ì—ì„œ ìƒì„±í•œ ScreenGuië“¤ì´ ë‚¨ì•„ìˆë‹¤ë©´ ì•ˆì „í•˜ê²Œ ì •ë¦¬
        for _, gui in ipairs(lp.PlayerGui:GetChildren()) do
            if gui:IsA("ScreenGui") then
                if gui.Name == "ServerInfoGui"
                or gui.Name == "HelpGui"
                or gui.Name == "CmdBarGui"
                or gui.Name == "FriendNotification"
                or gui.Name == "PingNotification"
                or gui.Name == "FriendAlerts"
                or gui.Name == "FriendAlerts" then
                    pcall(function() gui:Destroy() end)
                end
            end
        end

        -- ì±„íŒ… ë¡œê·¸ ì—°ê²° í•´ì œ(ì¬í™•ì¸)
        for k, v in pairs(chatLogConnections) do
            if v then
                pcall(function() v:Disconnect() end)
            end
            chatLogConnections[k] = nil
        end
        chatLogActive = false

        print("ìŠ¤í¬ë¦½íŠ¸ê°€ ì •ìƒì ìœ¼ë¡œ ì¢…ë£Œë˜ì—ˆìŠµë‹ˆë‹¤.")
        return
    end
end

-- [ë“œë˜ê·¸ ê¸°ëŠ¥ í•¨ìˆ˜]
local function makeDraggable(frame)
    local dragging = false
    local dragInput
    local dragStart
    local startPos

    local function update(input)
        local delta = input.Position - dragStart
        frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end

    frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = frame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)

    frame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)

    RunService.RenderStepped:Connect(function()
        if dragging and dragInput then
            update(dragInput)
        end
    end)
end

local function createHelpGui()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "HelpGui"
    screenGui.Parent = lp:WaitForChild("PlayerGui")
    screenGui.ResetOnSpawn = false
    screenGui.DisplayOrder = 999999999
    screenGui.IgnoreGuiInset = true

    local frame = Instance.new("Frame")
    -- [ìˆ˜ì •] ê¸€ì í¬ê¸°ì— ë§ì¶° í”„ë ˆì„ í¬ê¸°ë¥¼ í‚¤ì›€
    frame.Size = UDim2.new(0, 360, 0, 780)
    frame.Position = UDim2.new(1, -370, 0.5, -310)
    frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    frame.BorderSizePixel = 0
    frame.Visible = false
    frame.Parent = screenGui

    makeDraggable(frame)

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = frame

    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 40)
    title.Text = "ëª…ë ¹ì–´ ëª©ë¡"
    title.TextColor3 = Color3.new(1, 1, 1)
    title.BackgroundTransparency = 1
    title.Font = Enum.Font.GothamBold
    title.TextSize = 18
    title.Parent = frame

    local list = Instance.new("TextLabel")
    list.Size = UDim2.new(1, -20, 1, -50)
    list.Position = UDim2.new(0, 15, 0, 50)
    
    -- ëª…ë ¹ì–´ ëª©ë¡ ì—…ë°ì´íŠ¸ (WalkFling ì¶”ê°€)
    list.Text = "â€¢ ; í‚¤ë¥¼ ëˆŒëŸ¬ ì»¤ë§¨ë“œë°” ì‹¤í–‰ ê°€ëŠ¥\nâ€¢ ì±„íŒ…ì°½ì—ì„œëŠ” ë°˜ë“œì‹œ . ì„ ë¶™ì—¬ì•¼ í•¨\n\nâ€¢ .ì¬ì ‘ / .rj : ì„œë²„ ì¬ì ‘ì†\nâ€¢ .ë ‰ / .lag : ì„œë²„ ë ‰ ì‹¤í–‰\nâ€¢ .ë„ì›€ / .help : ëª…ë ¹ì–´ ëª©ë¡\nâ€¢ .ì—ì„ë´‡ / .aimbot : ì—ì„ë´‡ ì‹¤í–‰\nâ€¢ .reset / .die : ìºë¦­í„° í™•ì‹¤íˆ ì£½ê¸°\nâ€¢ .í•‘ : í˜„ì¬ í•‘ í™•ì¸\nâ€¢ .ì¹œêµ¬ : ì„œë²„ ë‚´ ì¹œêµ¬ ìˆ˜ í™•ì¸\nâ€¢ .ì„œë²„ ì •ë³´ : ì„œë²„ ì •ë³´ GUI í‘œì‹œ\nâ€¢ .to [ì´ë¦„] : ëŒ€ìƒì—ê²Œ ì´ë™\nâ€¢ .join [ì´ë¦„] : ì¹œêµ¬ ë”°ë¼ê°€ê¸°\nâ€¢ .age [ì´ë¦„] : ê³„ì • ìƒì„±ì¼ ë° ë‚˜ì´ í™•ì¸\nâ€¢ .view [ì´ë¦„] : 3ì¸ì¹­ ê´€ì „ ëª¨ë“œ\nâ€¢ .unview : ê´€ì „ í•´ì œ (ë³¸ì¸ ì‹œì  ë³µêµ¬)\nâ€¢ .fullbright / .fb : ë§µ ì•„ì£¼ ë°ê²Œ\nâ€¢ .unfb : ë§µ ë°ê¸° ì›ë˜ëŒ€ë¡œ ë³µêµ¬\nâ€¢ .nofog : ì•ˆê°œ ì œê±°\nâ€¢ .infjump : ë¬´í•œ ì í”„ í† ê¸€\nâ€¢ .uninfjump : ë¬´í•œ ì í”„ í•´ì œ\nâ€¢ .noclip : ë²½ í†µê³¼ ì¼œê¸°\nâ€¢ .unnoclip : ë²½ í†µê³¼ ë„ê¸°\nâ€¢ .walkfling / .wf : ê±¸ì–´ì„œ ë‚ ë¦¬ê¸°(Noclip ì¼œì§)\nâ€¢ .unwalkfling / .unwf : ê±¸ì–´ì„œ ë‚ ë¦¬ê¸° ë„ê¸°\nâ€¢ .fly [ì†ë„] : ì¼ë°˜ ë‚ ê¸°\nâ€¢ .unfly : ì¼ë°˜ ë‚ ê¸° í•´ì œ\nâ€¢ .vfly [ì†ë„] : ì°¨ëŸ‰ ë° ìºë¦­í„° ìë™ ë‚ ê¸°\nâ€¢ .unvfly : ì°¨ëŸ‰ ë‚ ê¸° í•´ì œ\nâ€¢ .thirdp : 3ì¸ì¹­ ëª¨ë“œ (Zoom 999)\nâ€¢ .firstp : 1ì¸ì¹­ ê³ ì • ëª¨ë“œ\nâ€¢ .jobid : í˜„ì¬ ì„œë²„ Job ID ë³µì‚¬\nâ€¢ .spawn : í˜„ì¬ ìœ„ì¹˜ ìŠ¤í° ì§€ì •\nâ€¢ .delspawn : ìŠ¤í° ì§€ì • ì‚­ì œ\nâ€¢ .chatlog / .cl : ì±„íŒ… ë¡œê·¸ ì›¹í›… ì „ì†¡ ON\nâ€¢ .unchatlog / .uncl : ì±„íŒ… ë¡œê·¸ ì „ì†¡ OFF\nâ€¢ .jg / .joinlog : ì…/í‡´ì¥ ë¡œê·¸ ì¼œê¸° (ìƒˆ ì›¹í›…)\nâ€¢ .unjg / .unjoinlog : ì…/í‡´ì¥ ë¡œê·¸ ë„ê¸°\nâ€¢ .dex : Dex Explorer ì‹¤í–‰\nâ€¢ .rspy : SimpleSpy V3 ì‹¤í–‰\nâ€¢ .exit : ê²Œì„ ì¢…ë£Œ\nâ€¢ .ì¢…ë£Œ / .end : ìŠ¤í¬ë¦½íŠ¸ ì¢…ë£Œ"
    list.TextColor3 = Color3.fromRGB(220, 220, 220)
    list.BackgroundTransparency = 1
    list.Font = Enum.Font.Gotham
    -- [ìˆ˜ì •] ê¸€ì í¬ê¸°
    list.TextSize = 13
    list.TextXAlignment = Enum.TextXAlignment.Left
    list.TextYAlignment = Enum.TextYAlignment.Top
    list.Parent = frame

    local closeBtn = Instance.new("TextButton")
    closeBtn.Size = UDim2.new(0, 30, 0, 30)
    closeBtn.Position = UDim2.new(1, -35, 0, 5)
    closeBtn.Text = "X"
    closeBtn.TextColor3 = Color3.new(1, 1, 1)
    closeBtn.BackgroundColor3 = Color3.fromRGB(220, 60, 60)
    closeBtn.Font = Enum.Font.GothamBold
    closeBtn.Parent = frame

    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 5)
    btnCorner.Parent = closeBtn

    closeBtn.MouseButton1Click:Connect(function()
        frame.Visible = false
    end)

    return frame, screenGui
end

-- ===========================
-- createServerInfoGui
-- ===========================
function createServerInfoGui()
    -- ì´ë¯¸ ìˆìœ¼ë©´ í† ê¸€
    if serverInfoGui and serverInfoGui.Parent then
        serverInfoGui.Enabled = not serverInfoGui.Enabled
        return
    end

    serverInfoGui = Instance.new("ScreenGui")
    serverInfoGui.Name = "ServerInfoGui"
    serverInfoGui.Parent = lp:WaitForChild("PlayerGui")
    serverInfoGui.ResetOnSpawn = false
    serverInfoGui.DisplayOrder = 999999995

    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(0, 420, 0, 220)
    -- ìœ„ì¹˜ë¥¼ ì˜¤ë¥¸ìª½ìœ¼ë¡œ ì•½ê°„ ì´ë™
    mainFrame.Position = UDim2.new(0.55, -210, 0.5, -110)
    mainFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
    mainFrame.BorderSizePixel = 0
    mainFrame.Parent = serverInfoGui

    -- ë‘¥ê·¼ ëª¨ì„œë¦¬
    local mainCorner = Instance.new("UICorner")
    mainCorner.CornerRadius = UDim.new(0, 12)
    mainCorner.Parent = mainFrame

    -- ìƒë‹¨ í—¤ë”
    local header = Instance.new("Frame")
    header.Size = UDim2.new(1, 0, 0, 40)
    header.Position = UDim2.new(0, 0, 0, 0)
    header.BackgroundTransparency = 1
    header.Parent = mainFrame

    -- íƒ€ì´í‹€
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, -20, 0, 36)
    title.Position = UDim2.new(0, 10, 0, 2)
    title.BackgroundTransparency = 1
    title.Text = "ì„œë²„ ì •ë³´"
    title.Font = Enum.Font.GothamBold
    title.TextSize = 20
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.TextXAlignment = Enum.TextXAlignment.Center
    title.Parent = header

    -- ë“œë˜ê·¸
    makeDraggable(mainFrame)

    -- êµ¬ë¶„ì„ 
    local sep = Instance.new("Frame")
    sep.Size = UDim2.new(1, -20, 0, 1)
    sep.Position = UDim2.new(0, 10, 0, 42)
    sep.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
    sep.BorderSizePixel = 0
    sep.Parent = mainFrame

    -- ì„œë²„ ì¸ì› ì •ë³´
    local playersMaxLabel = Instance.new("TextLabel")
    playersMaxLabel.Size = UDim2.new(0.5, -15, 0, 24)
    playersMaxLabel.Position = UDim2.new(0, 12, 0, 52)
    playersMaxLabel.BackgroundTransparency = 1
    playersMaxLabel.Font = Enum.Font.Gotham
    playersMaxLabel.TextSize = 15
    playersMaxLabel.TextColor3 = Color3.fromRGB(230, 230, 230)
    playersMaxLabel.TextXAlignment = Enum.TextXAlignment.Left
    playersMaxLabel.Text = "ì„œë²„ ìµœëŒ€ ì¸ì›: " .. tostring(Players.MaxPlayers)
    playersMaxLabel.Parent = mainFrame

    local playersCurrentLabel = Instance.new("TextLabel")
    playersCurrentLabel.Size = UDim2.new(0.5, -15, 0, 24)
    playersCurrentLabel.Position = UDim2.new(0.5, 3, 0, 52)
    playersCurrentLabel.BackgroundTransparency = 1
    playersCurrentLabel.Font = Enum.Font.Gotham
    playersCurrentLabel.TextSize = 15
    playersCurrentLabel.TextColor3 = Color3.fromRGB(230, 230, 230)
    playersCurrentLabel.TextXAlignment = Enum.TextXAlignment.Left
    playersCurrentLabel.Text = "í˜„ì¬ ì¸ì›: " .. tostring(#Players:GetPlayers())
    playersCurrentLabel.Parent = mainFrame

    -- ê° í•­ëª© ë¼ë²¨ ìƒì„± í•¨ìˆ˜
    local function createRow(yOffset, labelText, valueText)
        local leftLabel = Instance.new("TextLabel")
        leftLabel.Size = UDim2.new(0.45, -10, 0, 26)
        leftLabel.Position = UDim2.new(0, 12, 0, yOffset)
        leftLabel.BackgroundTransparency = 1
        leftLabel.Font = Enum.Font.Gotham
        leftLabel.TextSize = 14
        leftLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
        leftLabel.TextXAlignment = Enum.TextXAlignment.Left
        leftLabel.Text = labelText
        leftLabel.Parent = mainFrame

        local valueLabel = Instance.new("TextLabel")
        valueLabel.Size = UDim2.new(0.35, -10, 0, 26)
        valueLabel.Position = UDim2.new(0.45, 0, 0, yOffset)
        valueLabel.BackgroundTransparency = 1
        valueLabel.Font = Enum.Font.Gotham
        valueLabel.TextSize = 14
        valueLabel.TextColor3 = Color3.fromRGB(230, 230, 230)
        valueLabel.TextXAlignment = Enum.TextXAlignment.Left
        valueLabel.Text = valueText
        valueLabel.Parent = mainFrame

        local copyBtn = Instance.new("TextButton")
        copyBtn.Size = UDim2.new(0.15, -10, 0, 24)
        copyBtn.Position = UDim2.new(0.8, 0, 0, yOffset)
        copyBtn.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        copyBtn.BorderSizePixel = 0
        copyBtn.Font = Enum.Font.Gotham
        copyBtn.TextSize = 13
        copyBtn.TextColor3 = Color3.fromRGB(255,255,255)
        copyBtn.Text = "Copy"
        copyBtn.AutoButtonColor = true
        copyBtn.Parent = mainFrame

        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 6)
        btnCorner.Parent = copyBtn

        return leftLabel, valueLabel, copyBtn
    end

    -- Place ID
    local _, placeValue, placeCopy = createRow(86, "Place id:", tostring(game.PlaceId))
    placeCopy.MouseButton1Click:Connect(function()
        pcall(function() setclipboard(tostring(game.PlaceId)) end)
        StarterGui:SetCore("SendNotification", {Title = "ë³µì‚¬ë¨", Text = "Place idê°€ ë³µì‚¬ë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 2})
    end)

    -- Job ID
    local _, jobValue, jobCopy = createRow(116, "Job id:", tostring(game.JobId))
    jobCopy.MouseButton1Click:Connect(function()
        pcall(function() setclipboard(tostring(game.JobId)) end)
        StarterGui:SetCore("SendNotification", {Title = "ë³µì‚¬ë¨", Text = "Job idê°€ ë³µì‚¬ë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 2})
    end)

    -- Game Name
    local success, gameInfo = pcall(function() return MarketplaceService:GetProductInfo(game.PlaceId) end)
    local gameNameStr = "Unknown"
    if success and gameInfo and type(gameInfo.Name) == "string" then
        gameNameStr = gameInfo.Name
    else
        if game.PlaceId and tostring(game.PlaceId) ~= "" then
            gameNameStr = tostring(game:GetService("HttpService") and (gameInfo and gameInfo.Name) or ("Place " .. tostring(game.PlaceId)))
        end
    end

    local _, nameValue, nameCopy = createRow(146, "Game Name:", gameNameStr)
    nameCopy.MouseButton1Click:Connect(function()
        pcall(function() setclipboard(tostring(gameNameStr)) end)
        StarterGui:SetCore("SendNotification", {Title = "ë³µì‚¬ë¨", Text = "GameNameì´ ë³µì‚¬ë˜ì—ˆìŠµë‹ˆë‹¤.", Duration = 2})
    end)

    -- Uptime
    local uptimeLabel = Instance.new("TextLabel")
    uptimeLabel.Size = UDim2.new(1, -24, 0, 26)
    uptimeLabel.Position = UDim2.new(0, 12, 0, 178)
    uptimeLabel.BackgroundTransparency = 1
    uptimeLabel.Font = Enum.Font.Gotham
    uptimeLabel.TextSize = 14
    uptimeLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    uptimeLabel.TextXAlignment = Enum.TextXAlignment.Left
    uptimeLabel.Text = "ì„œë²„ ì˜¤í”ˆ ì‹œê°„: 00:00:00"
    uptimeLabel.Parent = mainFrame

    -- ë‹«ê¸° ë²„íŠ¼
    local closeBtn = Instance.new("TextButton")
    closeBtn.Size = UDim2.new(0, 30, 0, 30)
    closeBtn.Position = UDim2.new(1, -40, 0, 6)
    closeBtn.Text = "X"
    closeBtn.Font = Enum.Font.GothamBold
    closeBtn.TextSize = 16
    closeBtn.TextColor3 = Color3.fromRGB(255,255,255)
    closeBtn.BackgroundColor3 = Color3.fromRGB(180,60,60)
    closeBtn.BorderSizePixel = 0
    closeBtn.Parent = mainFrame
    local closeCorner = Instance.new("UICorner")
    closeCorner.CornerRadius = UDim.new(0,6)
    closeCorner.Parent = closeBtn
    closeBtn.MouseButton1Click:Connect(function()
        serverInfoGui.Enabled = false
    end)

    -- ì‹¤ì‹œê°„ ê°±ì‹ 
    local startTick = startTime
    local hbConn
    hbConn = RunService.Heartbeat:Connect(function()
        if not (serverInfoGui and serverInfoGui.Parent) then
            if hbConn then hbConn:Disconnect() end
            return
        end

        playersCurrentLabel.Text = "í˜„ì¬ ì¸ì›: " .. tostring(#Players:GetPlayers())

        local elapsed = tick() - startTick
        local hours = math.floor(elapsed / 3600)
        local mins = math.floor((elapsed % 3600) / 60)
        local secs = math.floor(elapsed % 60)
        local hh = tostring(hours)
        local mm = (mins < 10) and ("0"..tostring(mins)) or tostring(mins)
        local ss = (secs < 10) and ("0"..tostring(secs)) or tostring(secs)
        uptimeLabel.Text = "ì„œë²„ ì˜¤í”ˆ ì‹œê°„: " .. hh .. ":" .. mm .. ":" .. ss
    end)
end

-- [ìš°ì¸¡ ì¤‘ì•™ ì•Œë¦¼ í•¨ìˆ˜]
local function showFriendAlert(text, color)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 250, 0, 50)
    frame.Position = UDim2.new(1, 260, 0.5, 0)
    frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    frame.BorderSizePixel = 0
    frame.Parent = friendAlertGui

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = frame

    local stripe = Instance.new("Frame")
    stripe.Size = UDim2.new(0, 5, 1, 0)
    stripe.Position = UDim2.new(0, 0, 0, 0)
    stripe.BackgroundColor3 = color
    stripe.BorderSizePixel = 0
    stripe.Parent = frame
    
    local stripeCorner = Instance.new("UICorner")
    stripeCorner.CornerRadius = UDim.new(0, 8)
    stripeCorner.Parent = stripe

    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, -20, 1, 0)
    label.Position = UDim2.new(0, 15, 0, 0)
    label.Text = text
    label.TextColor3 = Color3.new(1, 1, 1)
    label.BackgroundTransparency = 1
    label.Font = Enum.Font.GothamBold
    label.TextSize = 14
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.TextWrapped = true
    label.Parent = frame

    local tweenIn = TweenService:Create(frame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Position = UDim2.new(1, -260, 0.5, 0)})
    tweenIn:Play()

    task.delay(4, function()
        local tweenOut = TweenService:Create(frame, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {Position = UDim2.new(1, 260, 0.5, 0)})
        tweenOut:Play()
        tweenOut.Completed:Wait()
        frame:Destroy()
    end)
end

-- [ì»¤ë§¨ë“œ ë°” ìƒì„± í•¨ìˆ˜ (CMD Bar)]
local function createCmdBar()
    local gui = Instance.new("ScreenGui")
    gui.Name = "CmdBarGui"
    gui.Parent = lp:WaitForChild("PlayerGui")
    gui.ResetOnSpawn = false
    gui.DisplayOrder = 1000000000 -- ìµœìƒìœ„

    local bg = Instance.new("Frame")
    bg.Size = UDim2.new(0, 400, 0, 40)
    bg.Position = UDim2.new(0.5, -200, 0.7, 0) -- í™”ë©´ ì¤‘ì•™ë³´ë‹¤ ì•½ê°„ ì•„ë˜
    bg.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    bg.BackgroundTransparency = 0.3 -- ë°˜íˆ¬ëª…
    bg.BorderSizePixel = 0
    bg.Visible = false -- ì´ˆê¸°ì—” ìˆ¨ê¹€
    bg.Parent = gui

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = bg

    local input = Instance.new("TextBox")
    input.Size = UDim2.new(1, -20, 1, 0)
    input.Position = UDim2.new(0, 10, 0, 0)
    input.BackgroundTransparency = 1
    input.Text = ""
    input.PlaceholderText = "ëª…ë ¹ì–´ë¥¼ ì…ë ¥í•˜ì„¸ìš”... (ì˜ˆ: fb)"
    input.TextColor3 = Color3.new(1, 1, 1)
    input.PlaceholderColor3 = Color3.fromRGB(180, 180, 180)
    input.Font = Enum.Font.Gotham
    input.TextSize = 16
    input.Parent = bg

    input.FocusLost:Connect(function(enterPressed)
        if enterPressed then
            local txt = input.Text
            
            input.Text = ""
            bg.Visible = false

            if txt and #txt > 0 then
                if txt:sub(1, 1) ~= "." then
                    txt = "." .. txt
                end
                processCommand(txt)
            end
        end
        bg.Visible = false
    end)

    return bg, input
end

Players.PlayerAdded:Connect(function(player)
    if lp:IsFriendsWith(player.UserId) then
        showFriendAlert(player.DisplayName .. "ë‹˜ì´ ì„œë²„ì— ì ‘ì†í–ˆìŠµë‹ˆë‹¤!", Color3.fromRGB(0, 255, 100))
    end
    -- [Join Log ì²´í¬]
    if joinLogActive then
        sendJoinLeaveMessage(player, "Join")
    end
end)

Players.PlayerRemoving:Connect(function(player)
    if lp:IsFriendsWith(player.UserId) then
        showFriendAlert(player.DisplayName .. "ë‹˜ì´ ë‚˜ê°”ìŠµë‹ˆë‹¤.", Color3.fromRGB(255, 50, 50))
    end
    -- [Leave Log ì²´í¬]
    if joinLogActive then
        sendJoinLeaveMessage(player, "Leave")
    end
end)

-- Main ì‹¤í–‰ë¶€
local helpFrameGlobal, helpGuiGlobal = createHelpGui()
helpFrame = helpFrameGlobal -- ì „ì—­ ë³€ìˆ˜ë¡œ ì—°ê²° (processCommandì—ì„œ ì ‘ê·¼ ìœ„í•¨)
local cmdFrame, cmdInput = createCmdBar()

StarterGui:SetCore("SendNotification", {
    Title = "v4.4 ìŠ¤í¬ë¦½íŠ¸ í™œì„±í™”ë¨",
    Text = "; í‚¤ë¥¼ ëˆŒëŸ¬ ì»¤ë§¨ë“œë°”ë¥¼ ì—´ê±°ë‚˜ ì±„íŒ…ì— .helpë¥¼ ì…ë ¥í•˜ì„¸ìš”.",
    Duration = 7
})

chatConnection = lp.Chatted:Connect(function(msg)
    local cleanMsg = msg:gsub("^%s+", "") -- ì• ê³µë°± ì œê±°
    if cleanMsg:sub(1, 1) == "." then
        processCommand(cleanMsg)
    end
end)

-- ; í‚¤ ë°”ì¸ë”© ë¦¬ìŠ¤ë„ˆ
UserInputService.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.KeyCode == Enum.KeyCode.Semicolon then
        cmdFrame.Visible = not cmdFrame.Visible
        if cmdFrame.Visible then
            cmdInput.Text = ""
            task.wait()
            cmdInput:CaptureFocus()
        end
    end
end)

print("ìŠ¤í¬ë¦½íŠ¸ê°€ ë¡œë”© ë˜ì—ˆìŠµë‹ˆë‹¤ --")
